</head>
<body>
	
	<div class="limiter">
		<div class="container-profile100">
			<div class="header">
                <div class="cover-image100">
                </div>
				<%- messages('Web Appendage Pages/message.ejs',locals) %>
                <div class="container error-page">
                    <img src="/img/404.png" class="error-img" alt="">
                    <h1><%=errorStatus%></h1>
                    <h2><%=errorHeading%></h2>
                    <button class="btn btn-primary" id="btn">Go Back To Login</button>
                </div>
            </div>
		</div>
	</div>
<!--===============================================================================================-->	
	<script src="/vendor/jquery/jquery-3.2.1.min.js"></script>
<!--===============================================================================================-->
	<script src="/vendor/bootstrap/js/bootstrap.min.js"></script>
<!--===============================================================================================-->
	<script src="/vendor/jquery/tilt.jquery.min.js"></script>
	<script >
		$('.js-tilt').tilt({
			scale: 1.1
		})
		$('#btn').on('click',(e)=>{
			window.location = '/login'
		})
	</script>
<!--===============================================================================================-->
	<script src="/js/main.js"></script>

</body>
</html>