      var ctx1 = document.getElementById("progressPerMonth").getContext("2d");
      var myChart1 = new Chart(ctx1, {
        type: "bar",
        data: {
          labels: [
            "January",
            "February",
            "March",
            "April",
            "May",
            "June",
            "July",
            "August",
            "September",
            "October",
            "November",
            "December"
          ],
          datasets: [
            {
              label: "Employee Progress",
              data: [],
              backgroundColor: [
                "rgba(255, 99, 132, 0.5)",
                "rgba(54, 162, 235, 0.5)",
                "rgba(255, 206, 86, 0.5)",
                "rgba(75, 192, 192, 0.5)",
                "rgba(153, 102, 255, 0.5)",
                "rgb(221, 170, 74,0.5)",
                "rgba(126, 175, 48, 0.5)",
                "rgba(54, 87, 54,0.3)",
                "rgba(116, 116, 243,0.3)",
                "rgba(250, 127, 83,0.3)",
                "rgba(83, 150, 250,0.4)",
                " rgba(211, 83, 250,0.4)"
              ],
              borderColor: [
                "rgba(255, 99, 132, 1)",
                "rgba(54, 162, 235, 1)",
                "rgba(255, 206, 86, 1)",
                "rgba(75, 192, 192, 1)",
                "rgba(153, 102, 255, 1)",
                "rgba(255, 159, 64, 1)",
                "rgba(126, 175, 48, 1)",
                "rgba(54, 87, 54,1)",
                "rgb(116, 116, 243, 1)",
                "rgba(250, 127, 83,1)",
                "rgba(83, 150, 250,1)",
                "rgba(211, 83, 250,1)"
              ],
              borderWidth: 1
            }
          ]
        },
        options: {
          responsive: true,
          maintainAspectRatio: false,

          scales: {
            y: {
              beginAtZero: true,
              suggestedMax: 100
            }
          }
        }
      });

      function changeFunc() {
        var data1 = [50, 60, 70, 80, 90, 100, 90, 80, 70, 60, 50, 40];
        var selectBox = document.getElementById("selectBox");
        var selectedValue = selectBox.options[selectBox.selectedIndex].value;
        var data2 = JSON.parse(`<%-JSON.stringify(array)%>`);
        var val = data2.find((obj) => obj.name === selectedValue).emplprogress;
        var sales = data2.find((obj) => obj.name === selectedValue).salesAvg;
        var revenue = data2.find(
          (obj) => obj.name === selectedValue
        ).TotalIncome;
        document.getElementById("salesAvg").innerHTML = sales;
        document.getElementById("revenue").innerHTML = revenue;
        myChart1.data.datasets[0].data = val;

        myChart1.update();
      }
    </script>

