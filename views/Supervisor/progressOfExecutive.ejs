<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Prgress Analytics</title>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <link
      rel="icon"
      type="image/ico"
      sizes="16x16"
      href="<%=url%>/favicon.ico"
    />
    <!--===============================================================================================-->
    <link
      rel="stylesheet"
      type="text/css"
      href="<%=url%>/vendor/bootstrap/css/bootstrap.css"
    />

    <!--===============================================================================================-->
    <link
      rel="stylesheet"
      type="text/css"
      href="<%=url%>/fonts-1/font-awesome-4.7.0/css/font-awesome.css"
    />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="<%=url%>/css/main.css" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="<%=url%>/css/style_2.css" />
    <link
      rel="stylesheet"
      type="text/css"
      href="<%=url%>/toastr/toastr.min.css"
    />

    <!--===============================================================================================-->
  </head>

  <body>
    <div class="limiter">
      <div class="container-profile100">
        <div class="header">
          <div class="cover-image100">
            <img src="<%=url%>/logo/logo.png" class="pep-logo" alt="" />
            <a href="#">
              <span class="fa fa-bars menu-button" onclick="openMenu()"></span>
            </a>
            <a href="<%=url%>/supervisor/notification">
              <span class="fa fa-bell menu-button">
                <span class="button__badge" style="display: none"></span>
              </span>
            </a>
            <nav id="menu">
              <ul>
                <% permissions.forEach(element=> { %>
                <li>
                  <a
                    href="<%=url%>/supervisor<%= element.controller%>/<%=info.uuid%>"
                  >
                    <span class="<%= element.icon%>"></span>
                    <%= element.permission_name %>
                  </a>
                </li>
                <% }); %>
                <li>
                  <a href="<%=url%>/supervisor/signout">
                    <span class="fa fa-sign-out"> </span> Log Out</a
                  >
                </li>
              </ul>
              <span class="fa fa-times close-menu" onclick="closeMenu()"></span>
            </nav>
          </div>

          <!-- --------------------- -->
          <div class="container saleup">
            <h4 class="areaheading mb-3 text-center">Progress Analytics</h4>
            <p>The Team progress, sales, revenue will be shown below.</p>

            <div class="text-center m-4 d-flex flex-row">
              <div class="input-group flex-row m-2 p-2">
                <div class="input-group-prepend">
                  <div class="input-group-text">
                    <input
                      type="radio"
                      id="progress_byCity"
                      name="searchType"
                      aria-label="Radio button for following text input"
                    />
                    <label for="">Show Progress by City</label>
                  </div>
                </div>
              </div>

              <div class="input-group flex-row m-2 p-2">
                <div class="input-group-prepend">
                  <div class="input-group-text"></div>
                  <input
                    type="radio"
                    id="progress_byTeamLead"
                    name="searchType"
                    aria-label="Radio button for following text input"
                  />
                  <label for=""> Show Progress by Team Lead</label>
                </div>
              </div>
            </div>

            <div class="d-flex flex-row w-100">
              <div class="p-2 bd-highlight">
                <select
                  class="empnames ml-1 mt-2 mb-3 text-right"
                  id="selectCity"
                  name="selectBox"
                  style="display: none"
                >
                  <option disabled selected value="null">Select City</option>
                  <% cityNames.forEach(city => { %>
                  <option value="<%= city.city_uuid %>">
                    <%= city.city_name %>
                  </option>
                  <% }) %>
                </select>
                <!-- --------------Spinner for loading of the data ------------------ -->
              </div>

              <div class="p-2 bd-highlight">
                <select
                  class="empnames ml-1 mt-2 mb-3 text-right"
                  id="selectTeamLead"
                  name="selectBox"
                  style="display: none"
                >
                  <option disabled selected value="null">
                    Select Team Member
                  </option>
                </select>
              </div>
            </div>
            <div class="row mb-2 mt-5">
              <div class="col">
                <div>
                  <canvas
                    id="activitiesPerMonth"
                    width="450"
                    height="150"
                  ></canvas>
                  <canvas id="agencyPerMonth" width="450" height="150"></canvas>
                  <canvas id="cancelPerMonth" width="450" height="150"></canvas>
                </div>
              </div>
            </div>
          </div>

          <!-- ---------------- -->
        </div>
      </div>
    </div>

    <!--===============================================================================================-->
    <script src="<%=url%>/js/jquery.min.js"></script>
    <script src="<%=url%>/bootstrap/js/bootstrap.min.js"></script>
    <!-- Toastr -->
    <!--===============================================================================================-->
    <script src="<%=url%>/vendor/jquery/tilt.jquery.min.js"></script>
    <script src="<%=url%>/toastr/toastr.min.js"></script>
    <script src="<%=url%>/js/chart.js "></script>

    <script>
      var activitiesPerMonth_ID = document
          .getElementById("activitiesPerMonth")
          .getContext("2d"),
        agencyPerMonth_ID = document
          .getElementById("agencyPerMonth")
          .getContext("2d"),
        cancelPerMonth_ID = document
          .getElementById("cancelPerMonth")
          .getContext("2d"),
        teamLead_Data = [];

      /**
       *
       * Setting the charts to those canvas
       */
      var activitiesPerMonth_Chart = new Chart(activitiesPerMonth_ID, {
        type: "line",
        data: {
          labels: [],
          datasets: [
            {
              label: "Actitivies Per Month",
              data: [],
              backgroundColor: [
                "rgba(255, 99, 132, 0.5)",
                "rgba(54, 162, 235, 0.5)",
                "rgba(255, 206, 86, 0.5)",
                "rgba(75, 192, 192, 0.5)",
                "rgba(153, 102, 255, 0.5)",
                "rgb(221, 170, 74,0.5)",
                "rgba(126, 175, 48, 0.5)",
                "rgba(54, 87, 54,0.3)",
                "rgba(116, 116, 243,0.3)",
                "rgba(250, 127, 83,0.3)",
                "rgba(83, 150, 250,0.4)",
                " rgba(211, 83, 250,0.4)"
              ],
              borderColor: [
                "rgba(255, 99, 132, 1)",
                "rgba(54, 162, 235, 1)",
                "rgba(255, 206, 86, 1)",
                "rgba(75, 192, 192, 1)",
                "rgba(153, 102, 255, 1)",
                "rgba(255, 159, 64, 1)",
                "rgba(126, 175, 48, 1)",
                "rgba(54, 87, 54,1)",
                "rgb(116, 116, 243, 1)",
                "rgba(250, 127, 83,1)",
                "rgba(83, 150, 250,1)",
                "rgba(211, 83, 250,1)"
              ],
              borderWidth: 1
            }
          ]
        },
        options: {
          plugins: {
            title: {
              display: true
              // text: "Custom Chart Title"
            }
          },
          scales: {
            y: {
              beginAtZero: true,
              suggestedMax: 100
            }
          }
        }
      });
      var agencyPerMonth_Chart = new Chart(agencyPerMonth_ID, {
        type: "bar",
        data: {
          labels: [],
          datasets: [
            {
              label: "Agency Per Month",
              data: [],
              backgroundColor: [
                "rgba(255, 99, 132, 0.5)",
                "rgba(54, 162, 235, 0.5)",
                "rgba(255, 206, 86, 0.5)",
                "rgba(75, 192, 192, 0.5)",
                "rgba(153, 102, 255, 0.5)",
                "rgb(221, 170, 74,0.5)",
                "rgba(126, 175, 48, 0.5)",
                "rgba(54, 87, 54,0.3)",
                "rgba(116, 116, 243,0.3)",
                "rgba(250, 127, 83,0.3)",
                "rgba(83, 150, 250,0.4)",
                " rgba(211, 83, 250,0.4)"
              ],
              borderColor: [
                "rgba(255, 99, 132, 1)",
                "rgba(54, 162, 235, 1)",
                "rgba(255, 206, 86, 1)",
                "rgba(75, 192, 192, 1)",
                "rgba(153, 102, 255, 1)",
                "rgba(255, 159, 64, 1)",
                "rgba(126, 175, 48, 1)",
                "rgba(54, 87, 54,1)",
                "rgb(116, 116, 243, 1)",
                "rgba(250, 127, 83,1)",
                "rgba(83, 150, 250,1)",
                "rgba(211, 83, 250,1)"
              ],
              borderWidth: 1
            }
          ]
        },
        options: {
          scales: {
            y: {
              beginAtZero: true,
              suggestedMax: 100
            }
          }
        }
      });
      var cancelPerMonth_Chart = new Chart(cancelPerMonth_ID, {
        type: "doughnut",
        data: {
          labels: [],
          datasets: [
            {
              label: "Cancelled Activities Per Month",
              data: [],
              backgroundColor: [
                "rgba(255, 99, 132, 0.5)",
                "rgba(54, 162, 235, 0.5)",
                "rgba(255, 206, 86, 0.5)",
                "rgba(75, 192, 192, 0.5)",
                "rgba(153, 102, 255, 0.5)",
                "rgb(221, 170, 74,0.5)",
                "rgba(126, 175, 48, 0.5)",
                "rgba(54, 87, 54,0.3)",
                "rgba(116, 116, 243,0.3)",
                "rgba(250, 127, 83,0.3)",
                "rgba(83, 150, 250,0.4)",
                " rgba(211, 83, 250,0.4)"
              ],
              borderColor: [
                "rgba(255, 99, 132, 1)",
                "rgba(54, 162, 235, 1)",
                "rgba(255, 206, 86, 1)",
                "rgba(75, 192, 192, 1)",
                "rgba(153, 102, 255, 1)",
                "rgba(255, 159, 64, 1)",
                "rgba(126, 175, 48, 1)",
                "rgba(54, 87, 54,1)",
                "rgb(116, 116, 243, 1)",
                "rgba(250, 127, 83,1)",
                "rgba(83, 150, 250,1)",
                "rgba(211, 83, 250,1)"
              ],
              borderWidth: 1
            }
          ]
        },
        options: {
          plugins: {
            title: {
              display: true,
              text: "Cancel Activities Per Month"
            }
          },
          scales: {
            y: {
              beginAtZero: true,
              suggestedMax: 100
            }
          },
          //this aspect ratio will make the height and width of the donut
          aspectRatio: 3
        }
      });

      $("#progress_byCity").change((e) => {
        if ($("#selectTeamLead").css("display", "block"))
          $("#selectTeamLead").css("display", "none");
        $("#selectCity").css("display", "block");
      });

      $("#progress_byTeamLead").change((e) => {
        if ($("#selectCity").css("display", "none"))
          $("#selectCity").css("display", "block");
        $("#selectTeamLead").css("display", "block");
      });

      $("#selectCity").change((e) => {
        const cityDataSet = JSON.parse(`<%-JSON.stringify(cityNames)%>`);
        if (
          cityValueValidater(
            cityDataSet,
            $("#selectCity").find(":selected").val()
          )
        ) {
          //making the ajax call to fetch the data from the server
          let cityUUID = $("#selectCity").find(":selected").val();
          $.ajax({
            type: "GET",
            url: `${window.location.origin}/getCityAnalytics/${cityUUID}/<%=info.uuid%>`,
            error: (error) => {
              if (error) {
                toastr.error(error);
                console.error(error);
              }
            },
            success: (response) => {
              if (response) {
                toastr.success(response.status);
                //displaying the activity per month
                activitiesPerMonth_Chart.data.labels =
                  response.activitiesPerMonth.map(
                    (activity) => activity.moonth + " " + activity.Year
                  );
                activitiesPerMonth_Chart.data.datasets[0].data =
                  response.activitiesPerMonth.map(
                    (activity) => activity.activitiesPerMonth
                  );
                activitiesPerMonth_Chart.update();
                //end of activity per month

                // //displaying the agency per month

                agencyPerMonth_Chart.data.labels = response.agencyCount.map(
                  (activity) => activity.moonth + " " + activity.Year
                );
                agencyPerMonth_Chart.data.datasets[0].data =
                  response.agencyCount.map((activity) => activity.agencyCount);
                agencyPerMonth_Chart.update();

                //end of agency per month

                //displaying the cancel activities per month

                cancelPerMonth_Chart.data.labels =
                  response.cancelledactivitiesPerMonth.map(
                    (activity) => activity.moonth + " " + activity.Year
                  );
                cancelPerMonth_Chart.data.datasets[0].data =
                  response.cancelledactivitiesPerMonth.map(
                    (activity) => activity.cancelledactivitiesPerMonth
                  );
                cancelPerMonth_Chart.update();
                //end of cancel activities per month

                const selectTeamLead =
                  document.getElementById("selectTeamLead");
                response.teamLeadID.forEach((teamLead) => {
                  const option = document.createElement("option");
                  option.value = teamLead.team_L_uuid;
                  option.text = teamLead.team_L_name;
                  teamLead_Data.push(teamLead);
                  selectTeamLead.append(option);
                });
              }
            }
          });
        } else {
          toastr.error("Invalid City selected");
          return;
        }
      });

      const cityValueValidater = (dataSet, value) => {
        return dataSet.find((data) => data.city_uuid === value);
      };

      // ------------- Team Lead select on change ---------------------
      const teamLeadValidator = (teamLeadData, value) => {
        return teamLeadData.find((lead) => lead.team_L_uuid === value);
      };

      $("#selectTeamLead").change((e) => {
        if (
          teamLeadValidator(
            teamLead_Data,
            $("#selectTeamLead").find(":selected").val()
          )
        ) {
          //making the ajax call to fetch the data from the server
          let teamLeadUUID = $("#selectTeamLead").find(":selected").val();
          $.ajax({
            type: "GET",
            url: `${window.location.origin}/getTeamLeadAnalytics/${teamLeadUUID}/<%=info.uuid%>`,
            error: (error) => {
              if (error) {
                toastr.error(error.responseJSON.error);
                //displaying the activity per month
                activitiesPerMonth_Chart.data.labels = [];
                activitiesPerMonth_Chart.data.datasets[0].data = [];
                activitiesPerMonth_Chart.update();
                //end of activity per month
                // //displaying the agency per month
                agencyPerMonth_Chart.data.labels = [];
                agencyPerMonth_Chart.data.datasets[0].data = [];
                agencyPerMonth_Chart.update();
                //end of agency per month
                //displaying the cancel activities per month
                cancelPerMonth_Chart.data.labels = [];
                cancelPerMonth_Chart.data.datasets[0].data = [];
                cancelPerMonth_Chart.update();
                //  end of cancel activities per month
              }
            },
            success: (response) => {
              if (response) {
                toastr.success(response.status);
                //displaying the activity per month
                activitiesPerMonth_Chart.data.labels =
                  response.activitiesPerMonth.map(
                    (activity) => activity.moonth + " " + activity.Year
                  );
                activitiesPerMonth_Chart.data.datasets[0].data =
                  response.activitiesPerMonth.map(
                    (activity) => activity.activitiesPerMonth
                  );
                activitiesPerMonth_Chart.update();
                //end of activity per month
                // //displaying the agency per month
                agencyPerMonth_Chart.data.labels = response.agencyCount.map(
                  (activity) => activity.moonth + " " + activity.Year
                );
                agencyPerMonth_Chart.data.datasets[0].data =
                  response.agencyCount.map((activity) => activity.agencyCount);
                agencyPerMonth_Chart.update();
                //end of agency per month
                //displaying the cancel activities per month
                cancelPerMonth_Chart.data.labels =
                  response.cancelledactivitiesPerMonth.map(
                    (activity) => activity.moonth + " " + activity.Year
                  );
                cancelPerMonth_Chart.data.datasets[0].data =
                  response.cancelledactivitiesPerMonth.map(
                    (activity) => activity.cancelledactivitiesPerMonth
                  );
                cancelPerMonth_Chart.update();
                //  end of cancel activities per month
              } else {
                toastr.error("No response");
              }
            }
          });
        } else {
          toastr.error("Invalid City selected");
          return;
        }
      });
    </script>

    <script>
      var notificationCount = JSON.parse(
        "<%-JSON.stringify(unreadNotificationCount)%>"
      );
      if (notificationCount) {
        $(".button__badge").css("display", "block");
        $(".button__badge").html(
          JSON.parse("<%-JSON.stringify(unreadNotificationCount)%>")
        );
      }
      $(".js-tilt").tilt({
        scale: 1.1
      });

      //  teamMembers = JSON.parse(`%-JSON.stringify(teamMember)%>`),
      //   agencyCount = JSON.parse(`%-JSON.stringify(agencyCount)%>`),
      //   cancelledactivitiesPerMonth = JSON.parse(
      //     `%-JSON.stringify(cancelledactivitiesPerMonth)%>`
      //   ),
      //   activitiesPerMonth = JSON.parse(
      //     `%-JSON.stringify(activitiesPerMonth)%>`
      //   ),
      var menu = document.getElementById("menu");

      function openMenu() {
        menu.style.top = "0";
      }
      function closeMenu() {
        menu.style.top = "-100vh";
      }

    </script>
  </body>
</html>
