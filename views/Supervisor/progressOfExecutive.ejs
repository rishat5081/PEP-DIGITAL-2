    <!--===============================================================================================-->
  </head>

  <body>
    <div class="limiter">
      <div class="container-profile100">
        <div class="header">
          <div class="cover-image100">
            <img src="<%=url%>/logo/logo.png" class="pep-logo" alt="" />
            <a href="#">
              <span class="fa fa-bars menu-button" onclick="openMenu()"></span>
            </a>
            <a href="<%=url%>/supervisor/notification">
              <span class="fa fa-bell menu-button">
                <span class="button__badge" style="display: none"></span>
              </span>
            </a>
            <nav id="menu">
              <ul>
                <% permissions.forEach(element=> { %>
                <li>
                  <a
                    href="<%=url%>/supervisor<%= element.controller%>/<%=info.uuid%>"
                  >
                    <span class="<%= element.icon%>"></span>
                    <%= element.permission_name %>
                  </a>
                </li>
                <% }); %>
                <li>
                  <a href="<%=url%>/supervisor/signout">
                    <span class="fa fa-sign-out"> </span> Log Out</a
                  >
                </li>
              </ul>
              <span class="fa fa-times close-menu" onclick="closeMenu()"></span>
            </nav>
          </div>

          <!-- --------------------- -->
          <div class="container saleup">
            <h4 class="areaheading mb-3 text-center">Progress Analytics</h4>
            <p>The Team progress, sales, revenue will be shown below.</p>

            <div class="text-center m-4 d-flex flex-row">
              <div class="input-group flex-row m-2 p-2">
                <div class="input-group-prepend">
                  <div class="input-group-text">
                    <input
                      type="radio"
                      id="progress_byCity"
                      name="searchType"
                      aria-label="Radio button for following text input"
                    />
                    <label for="">Show Progress by City</label>
                  </div>
                </div>
              </div>

              <div class="input-group flex-row m-2 p-2">
                <div class="input-group-prepend">
                  <div class="input-group-text"></div>
                  <input
                    type="radio"
                    id="progress_byTeamLead"
                    name="searchType"
                    aria-label="Radio button for following text input"
                  />
                  <label for=""> Show Progress by Team Lead</label>
                </div>
              </div>
            </div>

            <div class="d-flex flex-row w-100">
              <div class="p-2 bd-highlight">
                <select
                  class="empnames ml-1 mt-2 mb-3 text-right"
                  id="selectBox"
                  name="selectBox"
                  onchange="changeFunc()"
                  hidden
                >
                  <option disabled selected value="null">Select City</option>
                  <% cityNames.forEach(city => { %>
                  <option value="<%= city.city_uuid %>">
                    <%= city.city_name %>
                  </option>
                  <% }) %>
                </select>
              </div>
              <div class="p-2 bd-highlight">
                <select
                  class="empnames ml-1 mt-2 mb-3 text-right"
                  id="selectBox"
                  name="selectBox"
                  onchange="changeFunc()"
                  hidden
                >
                  <option disabled selected value="null">
                    Select Team Member
                  </option>
                  <% for (var i = 0; i < array.length; i++) { %>
                  <option value="<%= array[i].name %>">
                    <%= array[i].name %>
                  </option>

                  <% } %>
                </select>
              </div>
            </div>

            <div class="linear_bg mb-5 mt-5">
              <div class="row m-1 p-2">
                <div class="col-sm m-1">
                  <div class="card">
                    <div class="card-body m-0">
                      <h5 class="card-title text-center">Sales(in percent)</h5>
                      <h6
                        class="card-subtitle mb-2 text-center text-muted"
                        id="salesAvg"
                      >
                        0%
                      </h6>
                    </div>
                  </div>
                </div>
                <div class="col-sm m-1">
                  <div class="card">
                    <div class="card-body">
                      <h5 class="card-title text-center">Revenue</h5>
                      <h6
                        class="card-subtitle mb-2 text-center text-muted"
                        id="revenue"
                      >
                        $00
                      </h6>
                    </div>
                  </div>
                </div>
              </div>
            </div>

            <div class="row mb-2 mt-5">
              <div class="col">
                <div style="height: 500px">
                  <canvas id="progressPerMonth"></canvas>
                </div>
              </div>
            </div>
          </div>

          <!-- ---------------- -->
        </div>
      </div>
    </div>

    <!--===============================================================================================-->
    <script src="<%=url%>/js/jquery.min.js"></script>
    <script src="<%=url%>/bootstrap/js/bootstrap.min.js"></script>
    <!-- Toastr -->
    <!--===============================================================================================-->
    <script src="<%=url%>/vendor/jquery/tilt.jquery.min.js"></script>
    <script src="<%=url%>/toastr/toastr.min.js"></script>
    <script src="<%=url%>/js/chart.js "></script>

